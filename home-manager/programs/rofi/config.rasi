/* Dark theme. */
@import "~/.cache/wal/colors-rofi-dark"

* {
  font: "FantasqueSansM Nerd Font 12";
}
window {
  anchor: north;
  location: north;
  width: 100%;
  padding: 4px;
  children: [ horibox ];
}
horibox {
  orientation: horizontal;
  children: [ prompt, entry, listview ];
}
listview {
  layout: horizontal;
  spacing: 5px;
  lines: 100;
}
entry {
  expand: false;
  width: 10em;
}
element-text, element-icon {
  background-color: inherit;
  text-color: inherit;
}
